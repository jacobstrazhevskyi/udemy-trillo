{
  "name": "trillo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:html": "linthtml src/**/*.html",
    "lint:css": "stylelint **/*.scss",
    "lint:js": "eslint ./src",
    "lint": "concurrently \"npm run lint:html\" \"npm run lint:css\" \"npm run lint:js\"",
    "build": "rm -rf dist && parcel build ./src/index.html --public-url ./",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "watch:sass": "node-sass ./src/sass/main.scss ./src/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass ./src/sass/main.scss ./src/css/style.comp.css",
    "concat:css": "concat -o ./src/css/style.concat.css ./src/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' ./src/css/style.concat.css -o ./src/css/style.prefix.css",
    "compress:css": "node-sass ./src/css/style.prefix.css ./src/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "keywords": [],
  "author": "Jacob",
  "license": "ISC",
  "devDependencies": {
    "@linthtml/linthtml": "^0.9.0",
    "autoprefixer": "^10.4.14",
    "concat": "^1.0.3",
    "concurrently": "^7.4.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "gh-pages": "^4.0.0",
    "live-server": "^1.2.2",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.3",
    "postcss-cli": "^10.1.0",
    "stylelint": "^14.12.1",
    "stylelint-config-standard-scss": "^5.0.0"
  }
}
